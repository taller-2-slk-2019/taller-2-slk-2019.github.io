responseUser:
    type: object
    properties:
        id: integer
        name: string
        email: string
        username: string
        picture: string
        updatedAt: datetime
        createdAt: datetime
        latitude?: number
        longitude?: number

responseUserRole:
    type: object
    properties:
        role: string
        updatedAt: datetime
        createdAt: datetime

responseUserWithRole:
    type: responseUser
    properties:
        userOrganizations: responseUserRole

responseOrganization:
    type: object
    properties:
        id: integer
        name: string
        picture: string
        latitude: number
        longitude: number
        description: string
        welcome: string
        updatedAt: datetime
        createdAt: datetime

responseOrganizationWithRole:
    type: responseOrganization
    properties:
        userOrganizations: responseUserRole

responseChannel:
    type: object
    properties:
        id: integer
        name: string
        description: string
        isPublic: boolean
        welcome: string
        updatedAt: datetime
        createdAt: datetime

responseMessage:
    type: object
    properties:
        id: integer
        type: string
        data: string
        channelId?: integer
        conversationId?: integer
        createdAt: datetime
        updatedAt: datetime
        sender: responseUser

responseForbiddenWord:
    type: object
    properties:
        id: integer
        word: string
        createdAt: string
        updatedAt: string
        organizationId: number

responseUserInvitation:
    type: object
    properties:
        token: string
        organization: responseOrganization
        invitedAt: datetime

responseConversation:
    type: object
    properties:
        id: integer
        users: responseUser[]

responseChannelStatistics:
    type: object
    properties:
        messageCount: integer
